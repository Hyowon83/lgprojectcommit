{
  "schemaVersion": "1.0.0",
  "imports": [
    "js/aw-popup2.directive",
    "js/aw-button.directive",
    "js/aw-panel.directive",
    "js/aw-panel-body.directive",
    "js/aw-panel-footer.directive",
    "css/interactionMatrix.css",
    "js/aw-flex-column.directive"
  ],
  "onEvent": [
    {
      "eventId": "ThemeChangeEvent",
      "action": "setTuiGridStyle"
    }
  ],
  "data": {},
  "actions": {
    "saveInteractionMatrix": {
      "actionType": "JSFunction",
      "method": "saveInteractionMatrix",
      "deps": "js/L2_ChecklistMatrixPopupSaveService"
    },
    "beforeClose":{
      "actionType": "JSFunction",
      "method": "beforeClose",
      "deps": "js/interactionMatrixService"
    },
    "setTuiGridStyle": {
      "actionType": "JSFunction",
      "method": "setTuiGridStyle",
      "inputData": {
        "ctx": "{{ctx}}"
      },
      "deps": "js/sodAPTableService"
    }
  },
  "i18n": {
    "save": ["lgepMasterViewMessages"],
    "interactionPopupDescRightTitle": ["lgepInteractionMatrixMessages"],
    "interactionTypeA": ["lgepInteractionMatrixMessages"],
    "interactionTypeB": ["lgepInteractionMatrixMessages"],
    "interactionTypeC": ["lgepInteractionMatrixMessages"],
    "interactionTypeD": ["lgepInteractionMatrixMessages"],
    "interactionTypeE": ["lgepInteractionMatrixMessages"],
    "interactionTypeAB": ["lgepInteractionMatrixMessages"],
    "interactionTypeAC": ["lgepInteractionMatrixMessages"],
    "interactionTypeAD": ["lgepInteractionMatrixMessages"],
    "cancel": ["lgepInteractionMatrixMessages"]
  }
}
