  "conditions": {
    "dfmealocation": {
      "expression": "ctx.locationContext['ActiveWorkspace:SubLocation'] === 'dchecklist'"
    },
    "fmealocation": {
      "expression": "ctx.locationContext['ActiveWorkspace:Location'] === 'fmasterlocation'"
    },
    "addDeleteVisible": {
      "expression": "conditions.fmealocation && ctx.fmea_table_init"
    },
    "addEnable": {
      "expression": "conditions.fmealocation && ctx.fmea_editing !== true && ctx.fmea_popup !== true"
    },
    "editVisible": {
      "expression": "conditions.fmealocation && ctx.fmea_row_select && ctx.fmea_editing !== true && ctx.fmea_popup !== true"
    },
    "editCancelSaveVisible": {
      "expression": "conditions.fmealocation && ctx.fmea_editing === true && ctx.fmea_popup !== true"
    },
    "deleteEnable": {
      "expression": "conditions.fmealocation && ctx.fmea_row_select && ctx.fmea_editing !== true && !(ctx.sidenavCommandId) && ctx.fmea_popup !== true"
    }
  },
