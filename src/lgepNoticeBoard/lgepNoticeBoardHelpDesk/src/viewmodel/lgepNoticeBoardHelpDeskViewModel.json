    "qnaLinkEventAction": {
      "actionType": "JSFunction",
      "method": "qnaLinkEventAction",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/lgepNoticeBoardHelpDeskService"
    },
    "qnaLinkAction": {
