  "actions": {
    "setAnswerListStyle": {
      "actionType": "JSFunction",
      "method": "setAnswerListStyle",
      "inputData": {
        "data": "{{data}}",
        "ctx": "{{ctx}}"
      },
      "deps": "js/lgepNoticeBoardHelpDeskService"
    },
    "setAnswerListBgStyle": {
      "actionType": "JSFunction",
      "method": "setAnswerListBgStyle",
      "inputData": {
        "data": "{{data}}",
        "ctx": "{{ctx}}"
      },
      "deps": "js/lgepNoticeBoardHelpDeskService"
