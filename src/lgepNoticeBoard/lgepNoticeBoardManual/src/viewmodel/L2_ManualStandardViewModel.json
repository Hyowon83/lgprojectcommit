{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-icon-button.directive",
        "js/aw-flex-row.directive",
        "js/aw-button.directive",
        "js/aw-flex-column.directive",
        "js/aw-sidenav.directive",
        "js/visible-when.directive",
        "js/aw-slider.directive",
        "js/aw-splitter.directive",
        "js/aw-checkbox.directive",
        "js/aw-separator.directive"
    ],
    "onEvent": [
        {
            "eventId": "designStandardTreeTableData.selectionChangeEvent",
            "cacheEventData": true,
            "action": "bomlineTreeSetBat"
        },
        {
            "eventId": "treeLoadAction",
            "cacheEventData": true,
            "action": "designStdContentLoad"
        },
        {
            "eventId": "bomLineTree.treeNodeSelected",
            "cacheEventData": true,
            "action": "pageContentView"
        }
    ],
    "actions": {
        "urlBookUidMapping": {
            "actionType": "JSFunction",
            "method": "urlBookUidMapping",
            "deps": "js/L2_DesignManualService"
        },
        "updateSliderValue": {
            "actionType": "JSFunction",
            "method": "updateSliderValue",
            "inputData": {
                "value": "{{prop.dbValue[0].sliderOption.value}}",
                "ctx": "{{ctx}}"
            },
            "deps": "js/L2_DesignManualService"
        },
        "backPage": {
            "actionType": "JSFunction",
            "method": "backPage",
            "deps": "js/L2_DesignManualService"
        },
        "frontPage": {
            "actionType": "JSFunction",
            "method": "frontPage",
            "deps": "js/L2_DesignManualService"
        },
        "summerNoteImageWidthMax": {
            "actionType": "JSFunction",
            "method": "summerNoteImageWidthMax",
            "inputData": {
                "data": "{{data}}"
            },
            "deps": "js/L2_DesignManualService"
        },
        "navModeTemp": {
            "actionType": "JSFunction",
            "method": "navModeTemp",
            "deps": "js/L2_DesignManualService"
        },
        "pageContentView": {
            "actionType": "JSFunction",
            "method": "pageContentView",
            "inputData": {
                "eventData": "{{data.eventData}}"
            },
            "deps": "js/L2_DesignManualService"
        },
        "selFolderCheck": {
            "actionType": "JSFunction",
            "method": "selFolderCheck",
            "outputData": {
                "folder": "folder"
            },
            "deps": "js/L2_DesignManualService"
        },
        "createDgnBookPopup": {
            "actionType": "JSFunction",
            "method": "show",
            "inputData": {
                "data": {
                    "declView": "L2_CreateManualBookPopup",
                    "locals": {
                        "caption": "{{i18n.createDgnBook}}",
                        "anchor": "closePopupAnchor"
                    },
                    "options": {
                        "reference": "referenceId",
                        "isModal": true,
                        "clickOutsideToClose": false,
                        "draggable": true,
                        "placement": "bottom-start",
                        "height": "210",
                        "width": "500"
                    }
                }
            },
            "events": {
                "success": [
                    {
                        "name": "createDgnBookPopupSuccess"
                    }
                ]
            },
            "deps": "js/popupService"
        },
        "attachPopupAction": {
            "actionType": "JSFunction",
            "method": "show",
            "inputData": {
                "data": {
                    "declView": "L2_PageAttachFileTable",
                    "locals": {
                        "caption": "{{i18n.manualAttach}}",
                        "anchor": "closePopupAnchor"
                    },
                    "options": {
                        "reference": "referenceId",
                        "isModal": true,
                        "clickOutsideToClose": false,
                        "placement": "bottom-start",
                        "draggable": true,
                        "height": "450",
                        "width": "800"
                    }
                }
            },
            "events": {
                "success": [
                    {
                        "name": "attachPopupActionSuccess"
                    }
                ]
            },
            "deps": "js/popupService"
        },
        "getUserList": {
            "actionType": "JSFunction",
            "method": "getUserList",
            "outputData": {
                "userList": "userList"
            },
            "deps": "js/L2_DesignManualService"
        },
        "getDataProvider": {
            "actionType": "dataProvider",
            "methods": [
                "postList"
            ]
        },
        "loadPostList": {
            "actionType": "JSFunction",
            "method": "loadPostList",
            "inputData": {
                "data": "{{data}}"
            },
            "outputData": {
                "searchPostResponse": "{{function:setPostList}}",
                "postTotalFound": "postTotalFound"
            },
            "deps": "js/L2_DesignManualService"
        },
        "bomlineTreeSet2": {
            "actionType": "JSFunction",
            "method": "bomlineTreeSet2",
            "inputData": {
                "data": "{{data}}"
            },
            "deps": "js/L2_DesignManualService"
        },
        "parameterCheck": {
            "actionType": "JSFunction",
            "method": "parameterCheck",
            "deps": "js/L2_DesignManualService"
        },
        "sidenavPushCommandRight": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [
                    {
                        "name": "awsidenav.openClose",
                        "eventData": {
                            "id": "sidenavPushRight",
                            "includeView": "L2_DesignStandardQnANav",
                            "commandId": "sidenavPushCommandRight"
                        }
                    }
                ]
            }
        },
        "treeNavPushLeftAction": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [
                    {
                        "name": "awsidenav.openClose",
                        "eventData": {
                            "id": "treeNavPushLeft",
                            "includeView": "L2_ManualStandardTreeNav",
                            "commandId": "treeNavPushLeftAction"
                        }
                    }
                ]
            },
            "events": {
                "success": [
                    {
                        "name": "treeNavPushLeftActionSuccess"
                    }
                ]
            }
        },
        "navModeBat": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "navModeTemp"
                },
                {
                    "action": "treeNavPushLeftAction"
                }
            ]
        },
        "designStdContentLoad": {
            "actionType": "JSFunction",
            "method": "designStdContentLoad",
            "inputData": {
                "ctx": "{{ctx}}",
                "data": "{{data}}"
            },
            "deps": "js/L2_DesignManualService"
        },
        "initialize": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "treeNavPushLeftAction"
                },
                {
                    "action": "parameterCheck"
                }
            ]
        },
        "createDesignBookBat": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "selFolderCheck"
                },
                {
                    "condition": "conditions.folder",
                    "action": "createDgnBookPopup"
                }
            ]
        },
        "bomlineTreeSetBat": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "bomlineTreeSet2"
                },
                {
                    "action": "urlBookUidMapping"
                }
            ]
        }
    },
    "data": {
        "chapterMergeView": {            
        "displayName": "{{i18n.chapterMergeView}}",
        "type": "BOOLEAN",
        "isRequired": "false",
        "isEditable": "true",
        "dbValue": "",
        "dispValue": "",
        "labelPosition": "PROPERTY_LABEL_AT_RIGHT"
        },
        "zoomSliderProp": {
            "isArray": "true",
            "dbValue": [
                {
                    "sliderOption": {
                        "disabled": false,
                        "min": 0.1,
                        "max": 2,
                        "step": 0.13,
                        "value": 1.04,
                        "orientation": "horizontal",
                        "range": false
                    },
                    "showIncrementButtons": true,
                    "sliderChangeEventAction": "updateSliderValue"
                }
            ]
        },
        "upArrow": {
            "action": "backPage",
            "iconName": "leftArrowIcon"
        },
        "downArrow": {
            "action": "frontPage",
            "iconName": "leftArrowIcon"
        },
        "upArrow2": {
            "iconName": "leftArrowIcon"
        },
        "downArrow2": {
            "iconName": "leftArrowIcon"
        },
        "revisionListBox": {
            "displayName": "",
            "type": "STRING",
            "isRequired": "true",
            "hasLov": "true",
            "dbValue": "",
            "dispValue": ""
        },
        "revisionListBoxValues": {
            "type": "STRING",
            "dbValue": []
        },
        "sideNavRightData": {
            "slide": "PUSH",
            "direction": "RIGHT_TO_LEFT",
            "animation": false,
            "width": "STANDARD",
            "height": "FULL"
        },
        "treeNavPushLeftData": {
            "slide": "PUSH",
            "direction": "LEFT_TO_RIGHT",
            "animation": false,
            "width": "STANDARD",
            "height": "FULL"
        },
        "summerNoteWidthMax": {
            "displayName": "{{i18n.summerNoteWidthMax}}",
            "type": "BOOLEAN",
            "isRequired": "false",
            "isEditable": "true",
            "dbValue": true,
            "dispValue": "{{i18n.summerNoteWidthMax}}",
            "labelPosition": "PROPERTY_LABEL_AT_SIDE"
        },
        "pageSize": 40
    },
    "dataProviders": {
        "postList": {
            "action": "loadPostList",
            "response": "{{data.searchPostResponse}}",
            "totalFound": "{{data.searchPostResponse.length}}"
        }
    },
    "columnProviders": {
        "searchColumn": {
            "frozenColumnIndex": -1,
            "columns": [
                {
                    "name": "object_name",
                    "displayName": "{{i18n.name}}",
                    "minWidth": 60,
                    "width": 150
                }
            ]
        }
    },
    "functions": {
        "setPostList": {
            "functionName": "setPostList",
            "parameters": [
                "{{data}}"
            ]
        }
    },
    "conditions": {
        "selValueConditions": {
            "expression": "data.selPage !== undefined"
        },
        "folder": {
            "expression": "data.folder===true"
        },
        "pageOpenConditions": {
            "expression": "data.pageOpenState === true"
        },
        "selectedBookCond": {
            "expression": "data.selectedBook != undefined && data.selectedTreeNodeType === true"
        },
        "selectedBookNoneManual": {
            "expression": "data.selectedBook != undefined"
        }
    },
    "i18n": {
        "summerNoteWidthMax": [
            "lgepDesignSequenceMessages"
        ],
        "treeShowTooltip": [
            "lgepDesignSequenceMessages"
        ],
        "tabShowTooltip": [
            "lgepDesignSequenceMessages"
        ],
        "leftSelectPlease": [
            "L2_DesignStandardMessages"
        ],
        "viewDetails": [
            "L2_DesignStandardMessages"
        ],
        "mostPosts": [
            "lgepKnowldegeManageMessages"
        ],
        "treeViewShowOrHide": [
            "L2_DesignStandardMessages"
        ],
        "createDgnBook": [
            "L2_DesignStandardMessages"
        ],
        "FileAttach": [
            "L2_DesignStandardMessages"
        ],
        "manualAttach": [
            "L2_DesignStandardMessages"
        ],
        "chapterMergeView": [
            "L2_DesignStandardMessages"
        ]
    },
    "lifecycleHooks": {
        "onInit": "initialize"
    }
}