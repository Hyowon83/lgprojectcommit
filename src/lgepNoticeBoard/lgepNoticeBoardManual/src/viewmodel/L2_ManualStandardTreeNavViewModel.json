{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-command-panel.directive",
        "js/aw-include.directive",
        "js/exist-when.directive",
        "js/visible-when.directive"
    ],
    "onEvent": [
        {
            "eventId": "manualPageSearchedWordList.selectionChangeEvent",
            "cacheEventData": true,
            "action": "listPageSearching"
        },
        {
            "eventId": "backArrowAction",
            "action": "backArrow"
        },
        {
            "eventId": "forwardArrowAction",
            "action": "forwardArrow"
        },
        {
            "eventId": "bookmarkAction",
            "action": "bookmark"
        },
        {
            "eventId": "backArrowBookmarkAction",
            "action": "backArrowBookmark"
        },
        {
            "eventId": "bomLineTree.treeNodeSelected",
            "cacheEventData": true,
            "action": "selectedTreeNode"
        },
        {
            "eventId": "treeViewChange",
            "cacheEventData": true,
            "action": "treeViewChangeAc"
        },
        {
            "eventId": "reviseEvent",
            "cacheEventData": true,
            "action": "revisePage"
        },
        {
            "eventId": "pageAddCmdAction",
            "cacheEventData": true,
            "action": "pageModeBat"
        },
        {
            "eventId": "chapterAddCmdAction",
            "cacheEventData": true,
            "action": "chapterModeBat"
        },
        {
            "eventId": "deleteCmdAction",
            "cacheEventData": true,
            "action": "deleteCmdAction"
        },
        {
            "eventId": "editCmdAction",
            "cacheEventData": true,
            "action": "editDesignPageBat"
        },
        {
            "eventId": "viewDetailEvent",
            "cacheEventData": true,
            "action": "viewDetailChangeAc"
        },
        {
            "eventId": "backArrowViewDetailAction",
            "action": "backArrowViewDetailAc"
        },
        {
            "eventId": "addFolderCmdAction",
            "action": "addMainFolderPopup"
        },
        {
            "eventId": "pageSearchCmdAction",
            "action": "pageSearchModeStart"
        }
    ],
    "actions": {
        "listPageSearching": {
            "actionType": "JSFunction",
            "method": "listPageSearching",
            "inputData": {
                "eventData": "{{data.eventData}}"
            },
            "deps": "js/L2_DesignManualService"
        },
        "useTextSearch": {
            "actionType": "JSFunction",
            "method": "useTextSearch",
            "inputData": {
                "data": "{{data}}",
                "ctx": "{{ctx}}"
            },
            "deps": "js/L2_DesignManualPopupService"
        },
        "pageSearchModeStart": {
            "actionType": "JSFunction",
            "method": "pageSearchModeStart",
            "deps": "js/L2_DesignManualService"
        },
        "addMainFolderPopup": {
            "actionType": "JSFunction",
            "method": "show",
            "inputData": {
                "data": {
                    "declView": "L2_ManualFolderCreate",
                    "locals": {
                        "caption": "{{i18n.folderAdd}}",
                        "anchor": "closePopupAnchor"
                    },
                    "options": {
                        "reference": "referenceId",
                        "isModal": true,
                        "clickOutsideToClose": false,
                        "draggable": true,
                        "placement": "bottom-start",
                        "height": "210",
                        "width": "400"
                    }
                }
            },
            "deps": "js/popupService"
        },
        "chapterMode": {
            "actionType": "JSFunction",
            "method": "chapterMode",
            "outputData": {
                "createHeight": "createHeight",
                "createWidth": "createWidth"
            },
            "deps": "js/L2_DesignManualService"
        },
        "pageMode": {
            "actionType": "JSFunction",
            "method": "pageMode",
            "outputData": {
                "createHeight": "createHeight",
                "createWidth": "createWidth"
            },
            "deps": "js/L2_DesignManualService"
        },
        "backArrowViewDetailAc": {
            "actionType": "JSFunction",
            "method": "backArrowViewDetailAc",
            "deps": "js/L2_DesignManualService"
        },
        "viewDetailChangeAc": {
            "actionType": "JSFunction",
            "method": "viewDetailChangeAc",
            "deps": "js/L2_DesignManualService"
        },
        "treeNavSet": {
            "actionType": "JSFunction",
            "method": "treeNavSet",
            "inputData": {
                "data": "{{data}}"
            },
            "deps": "js/L2_DesignManualService"
        },
        "deleteCmdAction": {
            "actionType": "JSFunction",
            "method": "deleteCmdAction",
            "deps": "js/L2_DesignManualService"
        },
        "revisePage": {
            "actionType": "JSFunction",
            "method": "revisePage",
            "deps": "js/L2_DesignManualService"
        },
        "treeViewChangeAc": {
            "actionType": "JSFunction",
            "method": "treeViewChangeAc",
            "outputData": {
                "navMode": "navMode"
            },
            "events": {
                "success": [
                    {
                        "name": "treeLoadAction"
                    }
                ]
            },
            "deps": "js/L2_DesignManualService"
        },
        "selectedTreeNode": {
            "actionType": "JSFunction",
            "method": "selectedTreeNode",
            "inputData": {
                "eventData": "{{data.eventData}}"
            },
            "deps": "js/L2_DesignManualService"
        },
        "bomlineAdd": {
            "actionType": "JSFunction",
            "method": "bomlineAdd",
            "outputData": {
                "treeSelect": "treeSelect"
            },
            "deps": "js/L2_DesignManualService"
        },
        "bomlineEdit": {
            "actionType": "JSFunction",
            "method": "bomlineEdit",
            "outputData": {
                "treeSelect": "treeSelect"
            },
            "deps": "js/L2_DesignManualService"
        },
        "backArrowBookmark": {
            "actionType": "JSFunction",
            "method": "backArrowBookmark",
            "deps": "js/L2_DesignManualService"
        },
        "backArrow": {
            "actionType": "JSFunction",
            "method": "backArrow",
            "deps": "js/L2_DesignManualService"
        },
        "forwardArrow": {
            "actionType": "JSFunction",
            "method": "forwardArrow",
            "deps": "js/L2_DesignManualService"
        },
        "bookmark": {
            "actionType": "JSFunction",
            "method": "bookmark",
            "deps": "js/L2_DesignManualService"
        },
        "createDgnPagePopup": {
            "actionType": "JSFunction",
            "method": "show",
            "inputData": {
                "data": {
                    "declView": "L2_CreateManualPagePopup",
                    "locals": {
                        "caption": "{{i18n.createDgnPage}}",
                        "anchor": "closePopupAnchor"
                    },
                    "options": {
                        "reference": "referenceId",
                        "isModal": true,
                        "clickOutsideToClose": false,
                        "draggable": true,
                        "placement": "bottom-start",
                        "height": "{{data.createHeight}}",
                        "width": "{{data.createWidth}}"
                    }
                }
            },
            "events": {
                "success": [
                    {
                        "name": "createDgnPagePopupSuccess"
                    }
                ]
            },
            "deps": "js/popupService"
        },
        "pageModeBat": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "bomlineAdd"
                },
                {
                    "condition": "conditions.treeSelectState",
                    "action": "pageMode"
                },
                {
                    "condition": "conditions.treeSelectState",
                    "action": "createDgnPagePopup"
                }
            ]
        },
        "chapterModeBat": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "bomlineAdd"
                },
                {
                    "condition": "conditions.treeSelectState",
                    "action": "chapterMode"
                },
                {
                    "condition": "conditions.treeSelectState",
                    "action": "createDgnPagePopup"
                }
            ]
        },
        "createDesignPageBat": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "bomlineAdd"
                },
                {
                    "condition": "conditions.treeSelectState",
                    "action": "createDgnPagePopup"
                }
            ]
        },
        "editDgnPagePopup": {
            "actionType": "JSFunction",
            "method": "show",
            "inputData": {
                "data": {
                    "declView": "L2_EditManualPagePopup",
                    "locals": {
                        "caption": "{{i18n.editDgnPage}}",
                        "anchor": "closePopupAnchor"
                    },
                    "options": {
                        "reference": "referenceId",
                        "isModal": true,
                        "clickOutsideToClose": false,
                        "draggable": true,
                        "placement": "bottom-start",
                        "height": "800",
                        "width": "1300"
                    }
                }
            },
            "events": {
                "success": [
                    {
                        "name": "editgnPagePopupSuccess"
                    }
                ]
            },
            "deps": "js/popupService"
        },
        "editDgnChapterPopup": {
            "actionType": "JSFunction",
            "method": "show",
            "inputData": {
                "data": {
                    "declView": "L2_EditManualChapterPopup",
                    "locals": {
                        "caption": "{{i18n.editDgnPage}}",
                        "anchor": "closePopupAnchor"
                    },
                    "options": {
                        "reference": "referenceId",
                        "isModal": true,
                        "clickOutsideToClose": false,
                        "draggable": true,
                        "placement": "bottom-start",
                        "height": "200",
                        "width": "300"
                    }
                }
            },
            "events": {
                "success": [
                    {
                        "name": "editgnChapterPopupSuccess"
                    }
                ]
            },
            "deps": "js/popupService"
        },
        "editDesignPageBat": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "bomlineEdit"
                },
                {
                    "condition": "conditions.treeSelectState",
                    "action": "editDgnPagePopup"
                },
                {
                    "condition": "conditions.treeSelectStateChapter",
                    "action": "editDgnChapterPopup"
                }
            ]
        }
    },
    "data": {},
    "conditions": {
        "treeChangeCond": {
            "expression": "data.navMode == 'awTree'"
        },
        "bookmarkModeStatus": {
            "expression": "data.navMode == 'bookmark'"
        },
        "normalState": {
            "expression": "data.navMode === undefined"
        },
        "viewDetail": {
            "expression": "data.navMode === 'viewDetail'"
        },
        "treeSelectState": {
            "expression": "data.treeSelect === true"
        },
        "treeSelectStateChapter": {
            "expression": "data.treeSelect === 'ManualChapter'"
        }
    },
    "i18n": {
        "summerNoteWidthMax": [
            "lgepDesignSequenceMessages"
        ],
        "treeShowTooltip": [
            "lgepDesignSequenceMessages"
        ],
        "tabShowTooltip": [
            "lgepDesignSequenceMessages"
        ],
        "leftSelectPlease": [
            "L2_DesignStandardMessages"
        ],
        "viewDetails": [
            "L2_DesignStandardMessages"
        ],
        "mostPosts": [
            "lgepKnowldegeManageMessages"
        ],
        "dgnStandardTree": [
            "L2_DesignStandardMessages"
        ],
        "createDgnPage": [
            "L2_DesignStandardMessages"
        ],
        "editDgnPage": [
            "L2_DesignStandardMessages"
        ],
        "folderAdd": [
            "L2_DesignStandardMessages"
        ]
    },
    "lifecycleHooks": {
        "onInit": "treeNavSet"
    }
}