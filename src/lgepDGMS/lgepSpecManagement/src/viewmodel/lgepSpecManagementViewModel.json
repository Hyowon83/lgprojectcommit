      },
      "deps": "js/SpecManagementService"
    },
    "loadData": {
      "actionType": "JSFunction",
      "method": "parent",
      "inputData": {
        "data": "{{data}}",
        "ctx": "{{ctx}}"
      },
      "outputData": {
        "treeLoadResult": "{{function:loadEmployeeTreeTableData}}"
      },
      "deps": "js/SpecManagementService"
    },
    "getChildrenData": {
      "actionType": "JSFunction",
      "method": "getChildrenData",
