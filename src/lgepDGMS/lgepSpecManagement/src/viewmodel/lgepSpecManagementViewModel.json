        "ctx": "{{ctx}}"
      },
      "deps": "js/SpecManagementService"
    },
    "specAddPopup": {
      "actionType": "JSFunction",
      "method": "show",
      "inputData": {
        "data": {
          "declView": "popupSpecAdd",
          "locals": {
            "caption": "{{i18n.addPopup}}"
          },
          "options": {
            "reference": "addId",
            "isModal": true,
            "clickOutsideToClose": true,
            "draggable": true,
            "placement": "center",
            "width": 700,
            "height": 900
          }
        }
      },
      "outputData": {
        "popupId": "addId"
      },
      "deps": "js/popupService"
    },
    "applyData": {
      "actionType": "JSFunction",
      "method": "getChildrenData",
      "inputData": {
        "data": "{{data}}",
        "ctx": "{{ctx}}"
      },
      "outputData": {
        "filterFacetResults": "{{function:getFilterFacets}}"
      },
      "deps": "js/SpecManagementService"
    },
    "filterFacetBatchAction": {
      "actionType": "batchJob",
      "steps": [
        {
          "action": "getFilterFacetData"
        },
        {
          "action": "applyData"
        }
      ]
    },
    "clearSearch": {
      "actionType": "JSFunction",
      "method": "clearSearch",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/SpecManagementService"
    },
    "appendUidToUrl": {
      "actionType": "JSFunction",
      "method": "appendUidToUrl",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/SpecManagementService"
    },
    "clearBatchAction": {
      "actionType": "batchJob",
      "steps": [
        {
          "action": "clearSearch"
        },
        {
          "action": "searchSpecTable"
        }
      ]
    },
    "searchSpecTable": {
      "actionType": "JSFunction",
      "method": "searchSpecTable",
      "deps": "js/SpecManagementService",
      "inputData": {
        "data": "{{data}}",
        "ctx": "{{ctx}}",
        "eventTable": "{{data.eventMap['specManagementDataProvider.selectionChangeEvent']}}"
      }
    },
    "specDeleteObject": {
      "actionType": "JSFunction",
      "method": "specDeleteObject",
      "inputData": {
        "data": "{{data}}",
        "ctx": "{{ctx}}"
      },
      "deps": "js/SpecManagementService"
    },
    "divisionValue": {
      "actionType": "JSFunction",
      "method": "divisionValue",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/SpecManagementService"
    },
    "typeValue": {
      "actionType": "JSFunction",
      "method": "typeValue",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/SpecManagementService"
    },
    "makerValue": {
      "actionType": "JSFunction",
      "method": "makerValue",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/SpecManagementService"
    },
    "showCompareNoty": {
      "actionType": "JSFunction",
      "method": "showCompareNoty",
      "deps": "js/SpecManagementService"
    },
    "showAddNoty": {
      "actionType": "JSFunction",
      "method": "showAddNoty",
      "deps": "js/SpecManagementService"
    },
    "showDeleteNoty": {
      "actionType": "JSFunction",
      "method": "showDeleteNoty",
      "deps": "js/SpecManagementService"
    },
    "showSummaryNoty": {
      "actionType": "JSFunction",
      "method": "showSummaryNoty",
      "deps": "js/SpecManagementService"
    },
    "specSidePanel": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "awsidenav.openClose",
            "eventData": {
              "id": "specSidePanel",
              "includeView": "specSidePanel",
              "commandId": "",
              "config": {
                "width": "STANDARD",
                "height": "FULL"
              }
            }
          }
        ]
      }
    }
  },
  "data": {
