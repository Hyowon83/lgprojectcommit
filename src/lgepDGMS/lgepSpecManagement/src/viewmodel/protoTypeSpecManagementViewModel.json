    "js/aw-button.directive",
    "js/aw-checkbox.directive",
    "js/aw-flex-row.directive",
    "js/aw-flex-column.directive",
    "js/aw-include.directive",
    "js/aw-list.directive",
    "js/aw-panel-body.directive",
    "js/aw-panel-footer.directive",
    "js/aw-panel-section.directive",
    "js/aw-splitter.directive",
    "js/aw-splm-table.directive",
    "js/aw-tab-set.directive",
    "js/aw-textbox.directive"
  ],
  "onEvent": [
    {
      "eventId": "loadBOM.contentLoaded",
      "condition": "data.dataProviders.specMgmtTableDataProvider.selectedObjects.length == 1",
      "cacheEventData": true,
      "action": "loadBOM"
    },
    {
      "eventId": "specEvent.navigator",
      "cacheEventData": true,
      "action": "specNavigatorPopup"
    },
    {
      "eventId": "specManagement.compare",
      "cacheEventData": true,
      "action": "originCompare"
    },
    {
      "eventId": "specMgmtCheckBoxListReload",
      "cacheEventData": true,
      "action": "loadCheckboxBatchAction"
    },
    {
      "eventId": "specMgmtCheckBoxListReload",
      "cacheEventData": true,
      "action": "checkBoxDivision"
    },
    {
      "eventId": "selectCheckBoxEvent",
      "cacheEventData": true,
      "action": "actionList"
    },
    {
      "eventId": "aw.windowResize",
      "cacheEventData": true,
      "action": "setBoxWidth"
    }
  ],
