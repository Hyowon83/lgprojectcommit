  "actions": {
    "divisionCheckBoxLoad": {
      "actionType": "dataProvider",
      "method": "categoryDivision"
    },
    "actionList": {
      "actionType": "JSFunction",
      "method": "actionList",
      "deps": "js/protoTypeSpecManagementService"
    },
    "loadBOM": {
      "actionType": "JSFunction",
      "method": "loadBOM",
      "inputData": {
        "ctx": "{{ctx}}",
        "data": "{{data}}"
      },
      "deps": "js/protoTypeSpecManagementService"
    },
    "originCompare": {
      "actionType": "JSFunction",
      "method": "originCompare",
      "inputData": {
        "ctx": "{{ctx}}",
        "data": "{{data}}"
      },
      "deps": "js/protoTypeSpecManagementService"
    },
    "loadData": {
      "actionType": "JSFunction",
      "method": "specMgmtTable",
      "inputData": {
        "ctx": "{{ctx}}",
        "data": "{{data}}"
      },
      "outputData": {
        "result": "result",
        "totalFound": "totalFound",
        "filterResults": "{{function:filterRowsWithSort}}"
      },
      "deps": "js/protoTypeSpecManagementService"
    },
    "searchItem": {
      "actionType": "JSFunction",
      "method": "searchItem",
      "inputData": {
        "ctx": "{{ctx}}",
        "data": "{{data}}"
      },
      "deps": "js/protoTypeSpecManagementService"
    },
    "checkBoxDivision": {
      "actionType": "JSFunction",
      "method": "checkBoxDivision",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/protoTypeSpecManagementService"
    },
    "reload": {
      "actionType": "JSFunction",
      "method": "reload",
      "deps": "js/protoTypeSpecManagementService"
    },
    "technicalInitialization": {
      "actionType": "JSFunction",
      "method": "technicalInitialization",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/protoTypeSpecManagementService"
    },
    "marketingInitialization": {
      "actionType": "JSFunction",
      "method": "marketingInitialization",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/protoTypeSpecManagementService"
    },
    "selectCheckBoxEvent": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "selectCheckBoxEvent"
          }
        ]
      }
    },
    "navigatorEvent": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "specEvent.navigator"
          }
        ]
      }
    },
    "specNavigatorPopup": {
      "actionType": "JSFunction",
      "method": "show",
      "inputData": {
        "data": {
          "declView": "protoTypeSpecSidePopup",
          "locals": {
            "caption": "모델 네비게이터"
          },
          "options": {
            "reference": "navigatorId",
            "isModal": false,
            "clickOutsideToClose": false,
            "draggable": true,
            "placement": "left",
            "width": 680,
            "height": 795
          }
        }
      },
      "outputData": {
        "popupId": "navigatorId"
      },
      "deps": "js/popupService"
    },
    "popupLoadQnA": {
      "actionType": "JSFunction",
      "method": "show",
      "inputData": {
        "data": {
          "declView": "loadQnA",
          "locals": {
            "caption": "등록된 질문"
          },
          "options": {
            "reference": "questionId",
            "isModal": false,
            "clickOutsideToClose": false,
            "draggable": true,
            "placement": "right",
            "width": 680,
            "height": 795
          }
        }
      },
      "outputData": {
        "popupId": "questionId"
      },
      "deps": "js/popupService"
    },
    "loadCheckboxBatchAction": {
      "actionType": "batchJob",
      "steps": [
        {
          "action": "divisionCheckBoxLoad"
        }
      ]
    },
    "multiSelection": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "specMgmtTableDataProvider.multiSelectModeAction",
            "eventData": {
              "multiSelect": true
            }
          }
        ]
      }
    },
    "setBoxWidth": {
      "actionType": "JSFunction",
      "method": "setBoxWidth",
      "inputData": {
        "data": "{{data}}"
      },
      "deps": "js/protoTypeSpecManagementService"
    }
  },
  "dataProviders": {
    "specMgmtTableDataProvider": {
      "action": "loadData",
      "response": "{{data.filterResults}}",
      "totalFound": "{{data.totalFound}}",
      "selectionModelMode": "multiple"
    },
    "categoryDivision": {
      "dataProviderType": "Static",
      "response": [],
      "totalFound": 1,
      "selectionModelMode": "none"
    }
  },
  "columnProviders": {
    "specMgmtTableColumnProvider": {
      "frozenColumnIndex": -1,
      "columns": [
        {
          "name": "object_string",
          "displayName": "이름",
          "width": 300
        },
        {
          "name": "l2_division",
          "displayName": "부서",
          "width": 100
        },
        {
          "name": "l2_source",
          "displayName": "출처",
          "width": 100
        },
        {
          "name": "l2_model_name",
          "displayName": "모델명",
          "width": 100
        },
        {
          "name": "l2_issue_date",
          "displayName": "생성일",
          "width": 100
        }
      ]
    }
  },
  "grids": {
    "specMgmtTable": {
      "dataProvider": "specMgmtTableDataProvider",
      "columnProvider": "specMgmtTableColumnProvider",
      "addIconColumn": true,
      "gridOptions": {
        "enablePinning": false,
        "enableSorting": true
      }
    }
  },
  "functions": {
    "filterRowsWithSort": {
      "functionName": "filterRowsWithSort",
      "parameters": [
        "{{data.columnProviders.specMgmtTableColumnProvider.sortCriteria}}",
        "{{data.dataProviders.specMgmtTableDataProvider.startIndex}}",
        "{{data.pageSize}}"
      ]
    }
  },
  "i18n": {},
  "conditions": {
    "technicalSpec": {
      "expression": "data.selectedTab.tabKey === 'techPage'"
    },
    "marketingSpec": {
      "expression": "data.selectedTab.tabKey === 'marketingPage'"
    },
    "divisionDVZ": {
      "expression": "ctx.userSession.props.group.uiValue === 'DVZ'"
    },
    "divisionHnA": {
      "expression": "ctx.userSession.props.group.uiValue === 'H&A'"
    }
  },
  "lifecycleHooks": {
    "onInit": "multiSelection"
  }
}
