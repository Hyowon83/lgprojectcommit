{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-panel-body.directive",
        "js/aw-button.directive"
    ],
    "onEvent": [{
        "action" :"goToLink"
        }
    ],
    "data": {
    },
    "actions": {
        "navigationAction": {
            "actionType": "Navigate",
            "navigateIn": "newTab",
            "navigateTo": "",
            "navigationParams": {}
        },
        "goToLink":{
            "actionType": "JSFunction",
            "method": "goToLink",
            "inputData": {
                "ctx": "{{ctx}}",
                "data":"{{data}}"
            },
            "deps": "js/buttonForCheck"
        }
    },
    "i18n": {
        "open": ["lgepCommonMessages"]
      }
}