    "onEvent": [
    ],
    "conditions": {
        "listMode": {
            "expression": "ctx.checklist.browseMode == '0'"
        },
        "viewMode": {
            "expression": "ctx.checklist.browseMode == '1'"
        },
        "isChecklistEditing": {
            "expression": "ctx.checklist.browseMode == '1' && ctx._editing"
        },
        "isNotEditing": {
            "expression": "ctx.checklist.browseMode == '1' && !ctx._editing"
        },
        "isFreezed": {
            "expression": "ctx.checklist.browseMode == '1' && ctx.checklist.freezed == true"
        },
        "isNotFreezed": {
            "expression": "ctx.checklist.browseMode == '1' && ctx.checklist.freezed != true"
        }
    },
