    "productTypeText": {
      "displayName": "{{i18n.chkColumnProductType}}",
      "type": "STRING",
      "isRequired": "true",
      "isEditable": "true",
      "dbValue": "",
      "dispValue": ""
    },
    "productClassText": {
      "displayName": "{{i18n.chkColumnProductClass}}",
      "type": "STRING",
      "isRequired": "true",
      "isEditable": "true",
      "dbValue": "",
      "dispValue": ""
    },
    "isChecklistTarget": {
      "displayName": "{{i18n.isChecklistTarget}}",
      "type": "STRING",
      "isRequired": "true",
      "hasLov": "true",
      "dbValue": "Showcase",
      "dispValue": "Showcase"
    },
    "isChecklistTargetValues": {
      "type": "STRING",
