{
  "schemaVersion": "1.0.0",
  "imports": [
    "js/aw-command-panel.directive",
    "js/aw-panel.directive",
    "js/aw-panel-body.directive",
    "js/aw-textbox.directive",
    "js/aw-listbox.directive",
    "js/aw-button.directive"
  ],
  "data": {
    "disabledButtonChk": {
      "displayName": "",
      "type": "BOOLEAN",
      "isRequired": "false",
      "isEditable": "true",
      "dbValue": false
    },
    "structureNameText": {
      "displayName": "{{i18n.openChecklistTitle}}",
      "type": "STRING",
      "isRequired": "true",
      "isEditable": "true",
      "dbValue": "",
      "dispValue": ""
